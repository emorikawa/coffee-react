{
  "name": "coffee-react",
  "description": "Coffeescript compiler wrapper adding coffee-react-transform CJSX support",
  "keywords": [
    "coffeescript",
    "react",
    "jsx",
    "cjsx",
    "coffee-react"
  ],
  "author": "James Friend",
  "version": "1.0.3",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/jsdf/coffee-react/master/LICENSE"
    }
  ],
  "files": [
    "register.js",
    "bin/",
    "lib/"
  ],
  "main": "./lib/coffee-react-script",
