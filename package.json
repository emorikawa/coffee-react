  "homepage": "https://github.com/jsdf/coffee-react",
  "bugs": "https://github.com/jsdf/coffee-react/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsdf/coffee-react.git"
  },
  "dependencies": {
    "mkdirp": "~0.3.5",
    "coffee-script": "^1.7.1",
    "coffee-react-transform": "git://github.com/jsdf/coffee-react-transform.git#8ca145a4efdb73a503e6392bc841331da1a49c6c"
  },
  "devDependencies": {
    "concat-stream": "^1.4.5",
    "tap": "^0.4.9",
    "object.assign": "^0.5.0",
    "react": "^0.11.2"
  }
}
